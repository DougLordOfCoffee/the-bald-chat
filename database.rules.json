{
  "rules": {
    "channels": {
      ".read": true,
      ".write": "auth != null",
      ".indexOn": ["timestamp"]
    },
    "messages": {
      "$channelId": {
        "$messageId": {
          ".write": "auth != null && ( (!data.exists() && newData.exists()) || (data.child('uid').val() === auth.uid) || (auth.uid === 'shELHHG7NJPJqQ0aRb7NR3sPhpJ3') )"
        }
      }
    }
    "users": { "$uid": { ".read": "auth != null && auth.uid === $uid", ".write": "auth != null && auth.uid === $uid" } },
    "usernames": { ".read": true, ".write": "auth != null" }
  }
}
