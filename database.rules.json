{
  "rules": {
    "channels": {
      ".read": true,
      ".write": true
    },

    "messages": {
      "$channelId": {
        "$messageId": {
          ".read": true,
          ".write": true,
          ".validate": "(newData.exists()) || (auth != null && auth.uid === 'shELHHG7NJPJqQ0aRb7NR3sPhpJ3')"
        }
      }
    },

    "users": {
      ".read": true,
      ".write": true
    },

    "usernames": {
      ".read": true,
      ".write": true
    }
  }
}
