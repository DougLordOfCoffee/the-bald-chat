{
  "rules": {
    "channels": {
      ".read": true,
      ".write": "auth != null",
      ".indexOn": ["timestamp"]
    },
    "messages": {
      "$channelId": {
        "$messageId": {
          ".read": true,
          ".write": "(!data.exists() && newData.exists() && auth != null) || (data.exists() && auth != null && (data.child('uid').val() === auth.uid || auth.uid === 'shELHHG7NJPJqQ0aRb7NR3sPhpJ3'))",
          ".indexOn": ["timestamp"]
        }
      }
    },
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    "usernames": {
      ".read": true,
      ".write": "auth != null"
    }
  }
}